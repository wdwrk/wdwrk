{
	"compilerOptions": {
		"strict": true,
		"alwaysStrict": true,
		"moduleResolution": "node16",
		"removeComments": true,
		"pretty": true,
		"module": "Node16",
		"target": "ES2020",
		"lib": ["ESNext"],
		"sourceMap": true,
		"incremental": true,
		"noEmitHelpers": true,
		"importHelpers": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"verbatimModuleSyntax": true,
	}
}
