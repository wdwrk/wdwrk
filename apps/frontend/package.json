		"eslint-config-neon": "^0.1.57",
		"eslint-formatter-pretty": "^5.0.0",
		"prettier": "^3.1.1",
		"turbo": "^1.11.2",
		"typescript": "^5.3.3",
		"vercel": "^33.0.1"
	}
