{
	"name": "@wdwrk/frontend",
	"version": "1.0.0",
	"description": "wdwrk web UI",
	"scripts": {
		"build": "yarn build:next",
		"build:next": "next build",
		"preview": "next start",
		"dev": "yarn dev:next",
		"dev:next": "next dev",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext .mjs,.js,.cjs,.ts,.tsx --format=pretty",
